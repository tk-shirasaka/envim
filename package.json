{
  "name": "Envim",
  "main": "main/bundle.js",
  "version": "1.0.0",
  "description": "Neovim app build by electron",
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "release:linux": "webpack && electron-builder --linux appImage",
    "release:mac": "webpack && electron-builder --mac zip",
    "release:win": "webpack && electron-builder --win zip",
    "start": "npm run build && electron ."
  },
  "repository": "https://github.com/tk-shirasaka/envim",
  "keywords": [
    "Electron",
    "Neovim"
  ],
  "author": "tk.shirasaka@gmail.com",
  "license": "CC0-1.0",
  "build": {
    "appId": "local.nvim.envim"
  },
  "devDependencies": {
    "@types/msgpack-lite": "^0.1.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "css-loader": "^5.0.1",
    "electron": "^11.0.4",
    "electron-builder": "^22.8.0",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "ts-loader": "^8.0.11",
    "tslint": "^6.1.3",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "neovim": "^4.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
