{
  "name": "Envim",
  "main": "main/bundle.js",
  "version": "1.0.0",
  "description": "Neovim app build by electron",
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "release:linux": "electron-builder --linux appImage",
    "release:mac": "electron-builder --mac zip",
    "release:win": "electron-builder --win zip",
    "start": "npm run build && electron ."
  },
  "repository": "https://github.com/tk-shirasaka/envim",
  "keywords": [
    "Electron",
    "Neovim"
  ],
  "author": "tk.shirasaka@gmail.com",
  "license": "CC0-1.0",
  "build": {
    "appId": "local.nvim.envim"
  },
  "devDependencies": {
    "@types/msgpack-lite": "^0.1.7",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "css-loader": "^3.5.3",
    "electron": "^9.0.1",
    "electron-builder": "^22.7.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "neovim": "^4.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
